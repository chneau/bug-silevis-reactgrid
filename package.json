		"normalize.css": "^8.0.1",
		"react": "^18.3.1",
		"react-dom": "^18.3.1"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.2",
		"@rsbuild/core": "^1.0.6",
		"@rsbuild/plugin-react": "^1.0.2",
		"@types/react": "^18.3.9",
		"@types/react-dom": "^18.3.0",
		"typescript": "^5.6.2"
	}
}
